//java code for pattern
package gitcode;
import java.util.*;
public class Pattern4 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=n;j++) {
				char ch =(char)('A'+i+j-2);
				System.out.print(ch+ " ");
				
				ch++;
			}
			System.out.println();
			
		}
	}

}
//A B C D 
//B C D E 
//C D E F 
//D E F G 
